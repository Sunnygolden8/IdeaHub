<h1>Listing Users</h1>

<%= link_to 'New User', new_user_registration_path %>

<table class="table">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to( 'Sign in', new_user_session_path ) %>

