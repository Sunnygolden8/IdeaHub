
<div class="container"><%= link_to "New Project", new_project_path, class:"btn btn-default " %>

<h1>Projects</h1>

<table class="table">
  <tr>
    <th>Title</th>
	<th>Owner</th>
    <th>Status</th>
  </tr>

<% @projects.each do |project| %>
  <tr>

    <td><%= link_to project.title, project_path(project), class:"btn btn-default " %></td>
    <td><%= project.user.first_name %> <%= project.user.last_name%></td>
    <td><%= project.status %></td>

  </tr>
<% end %>
</table>

</div>
