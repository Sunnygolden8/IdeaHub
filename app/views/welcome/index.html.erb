<div class="container">
	<p></p>
		<h1>WELCOME</h1>
		<table class="table">
			<tr><td colspan="2"><h2>Your Projects</h2></td></tr>
				<% if user_signed_in?%>
				<% current_user.projects.each do |project| %>
				  <tr>

				    <td><%= link_to project.title, project_path(project), class:"btn btn-default " %> </td>
					<td><%= project.status %></td>
				  
				<% end %>
				<%else%> 
					<td>Please Sign In to View Your Projects</td>
				<%end%>
			</tr>
		</table>
	</div>
